{
    "name": "balotias/statamic-asset-metadata-importer",
    "description": "Automatically import and map EXIF/IPTC metadata from images to your Statamic asset fields on upload",
    "keywords": ["statamic", "statamic-cms", "statamic-addon", "metadata", "exif", "iptc", "asset", "image"],
    "homepage": "https://github.com/balotias/statamic-asset-metadata-importer",
    "license": "MIT",
    "authors": [
        {
            "name": "Matthias Balota",
            "homepage": "https://github.com/balotias",
            "role": "Developer"
        }
    ],
    "autoload": {
        "psr-4": {
            "Balotias\\StatamicAssetMetadataImporter\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Balotias\\StatamicAssetMetadataImporter\\Tests\\": "tests"
        }
    },
    "require": {
        "php": "^8.1",
        "statamic/cms": "^5.0",
        "miljar/php-exif": "^0.6.5",
        "spatie/temporary-directory": "^2.3"
    },
    "require-dev": {
        "orchestra/testbench": "^9.0 || ^10.0"
    },
    "config": {
        "allow-plugins": {
            "pixelfear/composer-dist-plugin": true
        }
    },
    "extra": {
        "statamic": {
            "name": "Asset Metadata Importer",
            "description": "Automatically import and map EXIF/IPTC metadata from images to your Statamic asset fields on upload"
        },
        "laravel": {
            "providers": [
                "Balotias\\StatamicAssetMetadataImporter\\ServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
